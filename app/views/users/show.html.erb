<div class="container">
  <h2>Find a dock near your destination.</h2>
  <div class="container">
   <div class="form">
      <%= form_tag searches_path, :method => :post do %>   
      <%= text_field_tag :address, params[:address], placeholder: 'street city and zip...'%>  
      <%= submit_tag "Go", :name => nil %>  
      <% end %>  
    </div> 
  </div>
  <ul>
    <li>your favorites:</li>
    <% @user.favorites.each do |favorite| %>
    <%= form_tag searches_path, :method => :post do %>
    <%= submit_tag "#{favorite.address}", :name => :address %>
    <% end %>
    <% end %>
  </ul>
</div>

